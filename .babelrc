{
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": true,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "@babel/runtime"
            }
        ]
    ],
    "presets": [
        ["@babel/preset-env", { "loose": true, "modules": false }],
        "@babel/preset-stage-2",
        "@babel/preset-react"
    ]
}
